{
	"compilerOptions": {
		"tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo",
		"target": "ES2020",
		"module": "commonjs",
		"rootDir": "src",
		"outDir": "dist/src",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"incremental": false
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}